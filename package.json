{
  "name": "server-starter",
  "version": "1.0.0",
  "description": "Express server template",
  "author": "Timur Badretdinov <destinerx@gmail.com>",
  "keywords": [
    "express",
    "starter"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git://github.com/Destiner/server-starter.git"
  },
  "type": "module",
  "scripts": {
    "serve": "ts-node --esm src",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier -c \"src/**/*.{json,ts}\"",
    "lint:eslint": "eslint \"src/**/*.ts\"",
    "typecheck": "npx tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "16.0.1",
    "express": "4.18.1"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.30.3",
    "@typescript-eslint/parser": "5.30.3",
    "eslint": "8.19.0",
    "eslint-import-resolver-typescript": "^3.1.4",
    "eslint-plugin-import": "^2.26.0",
    "prettier": "2.7.1",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  }
}

{
  "name": "server-starter",
  "version": "1.0.0",
  "description": "Express server template",
  "author": "Timur Badretdinov <destinerx@gmail.com>",
  "keywords": [
    "express",
    "starter"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git://github.com/Destiner/server-starter.git"
  },
  "type": "module",
  "scripts": {
    "serve": "ts-node --esm src",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier -c \"src/**/*.{json,ts}\"",
    "lint:eslint": "eslint \"src/**/*.ts\"",
    "typecheck": "npx tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "16.3.1",
    "express": "4.18.2"
  },
  "devDependencies": {
    "@types/express": "4.17.18",
    "@types/node": "20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "eslint": "8.52.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "prettier": "^3.0.3",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}
